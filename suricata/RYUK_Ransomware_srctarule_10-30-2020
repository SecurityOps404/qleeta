IDS rules - optimized for Suricata
#
# These NIDS rules contain some variables that need to exist in your configuration.
# Make sure you have set:
#
# $HOME_NET	- Your internal network range
# $EXTERNAL_NET - The network considered as outside
# $SMTP_SERVERS - All your internal SMTP servers
# $HTTP_PORTS   - The ports used to contain HTTP traffic (not required with suricata export)
# 
alert dns any any -> any any (msg: "MISP e1035 [type:OSINT,osint:lifetime="perpetual",osint:certainty="50",tlp:white,misp-galaxy:ransomware="Ryuk ransomware"] Hostname ns1.aaatus.com"; dns_query; content:"ns1.aaatus.com"; nocase; pcre: "/(^|[^A-Za-z0-9-\.])ns1\.aaatus\.com$/i";  classtype:trojan-activity; sid:6256471; rev:1; priority:2; reference:url,https://192.168.10.70/events/view/1035;) 
alert http $HOME_NET any -> $EXTERNAL_NET any (msg: "MISP e1035 [type:OSINT,osint:lifetime="perpetual",osint:certainty="50",tlp:white,misp-galaxy:ransomware="Ryuk ransomware"] Outgoing HTTP Hostname ns1.aaatus.com"; flow:to_server,established; content: "Host|3a| ns1.aaatus.com"; fast_pattern; nocase; http_header; pcre: "/(^|[^A-Za-z0-9-\.])ns1\.aaatus\.com[^A-Za-z0-9-\.]/Hi"; tag:session,600,seconds; classtype:trojan-activity; sid:6256472; rev:1; priority:2; reference:url,https://192.168.10.70/events/view/1035;) 
alert dns any any -> any any (msg: "MISP e1035 [type:OSINT,osint:lifetime="perpetual",osint:certainty="50",tlp:white,misp-galaxy:ransomware="Ryuk ransomware"] Hostname ns2.aaatus.com"; dns_query; content:"ns2.aaatus.com"; nocase; pcre: "/(^|[^A-Za-z0-9-\.])ns2\.aaatus\.com$/i";  classtype:trojan-activity; sid:6256481; rev:1; priority:2; reference:url,https://192.168.10.70/events/view/1035;) 
